{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "./fortuno-back/Dockerfile"
  },
  "deploy": {
    "workingDirectory": "/app"
  },
  "runtime": "V2",
  "numReplicas": 1,
  "startCommand": null,
  "sleepApplication": false,
  "restartPolicyType": "ON_FAILURE",
  "restartPolicyMaxRetries": 10
}